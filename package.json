{
	"name": "generative-db",
	"displayName": "Generative-DB",
	"description": "Uses generative AI to help generate database queries the currently connected database schema.",
	"publisher": "DillonFletcher",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0",
		"azdata": "*"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:generative-db.generateQuery"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "generative-db.generateQuery",
				"title": "Use ChatGpt to Generate Query"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "mkdir -p ./out | tsc -p ./ | cp ./.env ./out/.env",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"proposedapi": "node installTypings.js"
	},
	"devDependencies": {
		"@types/azdata": "^1.46.6",
		"@types/glob": "^7.2.0",
		"@types/json2md": "^1.5.4",
		"@types/mocha": "^9.1.1",
		"@types/node": "^13.13.52",
		"@types/vscode": "^1.91.0",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"azdata-test": "^1.2.0",
		"eslint": "^6.8.0",
		"glob": "^7.2.3",
		"mocha": "^9.2.2",
		"typescript": "^5.5.4"
	},
	"dependencies": {
		"dotenv": "^16.4.5",
		"json2md": "^2.0.1",
		"langchain": "^0.1.37",
		"mssql": "^10.0.4",
		"openai": "^4.53.2",
		"typeorm": "^0.3.20"
	}
}
